<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">

    <servlet>
        <servlet-name>GetProductsHomeServlet</servlet-name>
        <servlet-class>controller.GetProductsHomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetProductsServlet</servlet-name>
        <servlet-class>controller.GetProductsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetSingleProduct</servlet-name>
        <servlet-class>controller.GetSingleProduct</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterServlet</servlet-name>
        <servlet-class>controller.RegisterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>controller.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>controller.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddToCartServlet</servlet-name>
        <servlet-class>controller.AddToCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetCartServlet</servlet-name>
        <servlet-class>controller.GetCartServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoadDataServlet</servlet-name>
        <servlet-class>controller.LoadDataServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProductsServlet</servlet-name>
        <servlet-class>controller.admin.ProductsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EmailServlet</servlet-name>
        <servlet-class>controller.admin.EmailServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FilteringProductsAdmin</servlet-name>
        <servlet-class>controller.admin.FilteringProductsAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BookingServlet</servlet-name>
        <servlet-class>controller.admin.BookingAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfirmCheckoutServlet</servlet-name>
        <servlet-class>controller.ConfirmCheckoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryServlet</servlet-name>
        <servlet-class>controller.HistoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>HistoryAdmin</servlet-name>
        <servlet-class>controller.admin.HistoryAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DashBoardAdminServlet</servlet-name>
        <servlet-class>controller.admin.DashBoardAdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutAdminSerlvet</servlet-name>
        <servlet-class>controller.admin.LogoutAdminSerlvet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetProductsHomeServlet</servlet-name>
        <url-pattern>/GetProductsHomeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetProductsServlet</servlet-name>
        <url-pattern>/products</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetSingleProduct</servlet-name>
        <url-pattern>/product</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterServlet</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddToCartServlet</servlet-name>
        <url-pattern>/addToCart</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetCartServlet</servlet-name>
        <url-pattern>/GetCartServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoadDataServlet</servlet-name>
        <url-pattern>/LoadDataServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProductsServlet</servlet-name>
        <url-pattern>/productsAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EmailServlet</servlet-name>
        <url-pattern>/sendEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FilteringProductsAdmin</servlet-name>
        <url-pattern>/searchAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BookingServlet</servlet-name>
        <url-pattern>/bookingAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfirmCheckoutServlet</servlet-name>
        <url-pattern>/confirm-checkout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistoryServlet</servlet-name>
        <url-pattern>/history</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>HistoryAdmin</servlet-name>
        <url-pattern>/historyAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DashBoardAdminServlet</servlet-name>
        <url-pattern>/dashboardAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutAdminSerlvet</servlet-name>
        <url-pattern>/logoutAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>120</session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>LoadDataServlet</welcome-file>
    </welcome-file-list>
    <error-page>
        <error-code>404</error-code>
        <location>/errorPage.jsp</location>
    </error-page>
</web-app>
